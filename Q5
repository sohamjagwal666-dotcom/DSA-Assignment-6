/* Link list Node
struct Node
{
    int data;
    struct Node* next;

    Node(int x){
        data = x;
        next = NULL;
    }

};
*/

/* Should return true if linked list is circular, else false */
class Solution {
  public:
    bool isCircular(Node *head) {
        // Your code here
        Node* temp=head;
        temp=temp->next;
        while(temp!=NULL){
            if(temp==head){
                return true;
            }
            
            else{
                temp=temp->next;
                
            }
        }
        return false;
    }
};
